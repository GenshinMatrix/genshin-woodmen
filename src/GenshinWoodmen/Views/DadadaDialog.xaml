<local:ObservableContentDialog x:Class="GenshinWoodmen.Views.DadadaDialog"
                               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                               xmlns:local="clr-namespace:GenshinWoodmen.Views"
                               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                               Title="{DynamicResource CountSettings3}"
                               d:DesignHeight="NaN"
                               d:DesignWidth="NaN"
                               DefaultButton="Primary"
                               PrimaryButtonText="{DynamicResource ButtonStop}"
                               RenderOptions.BitmapScalingMode="Fant"
                               mc:Ignorable="d">
    <Grid>
        <Image Source="../../Resources/Audios/folder.jpg" Visibility="{Binding DefaultCoverVisibility}" />
        <Image Source="../../Resources/Audios/folder.en.jpg" Visibility="{Binding DefaultCoverVisibilityInverted}" />
    </Grid>
</local:ObservableContentDialog>
